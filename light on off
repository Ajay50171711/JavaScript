    <title>Document</title>
    <style>
        #box{
            height: 100px;
            width: 100px;
            border: 1px solid black;
            background-image: url(offbulb.png);
            background-size: contain;
        }
    </style>
</head>
<body>
    <div id="box"></div>
    <button onclick="bulbon()">On</button>
    <button onclick="bulboff()">Off</button>

    <script>
        function bulbon(){
            document.getElementById("box").style.backgroundImage = "url(blb.png)";
            }
            function bulboff(){
                document.getElementById("box").style.backgroundImage = "url(offbulb.png)";
                }
    </script>
</body>
